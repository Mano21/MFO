comandos latex

\documentclass[12pt]{article}

\usepackage[utf8]{inputenc}

\usepackage[T1]{fontenc}

\usepackage[portuguese]{babel} = lingua portuguesa, acentuação

\usepackage{lmodern,color,comment} = mexer com coloração,comentarios e 
linguagem

\usepackage{graphicx} = colcoar graficos e imagens


\begin{document} = inicio do documento
\end{document} = fim do documento

--> criando um bash
echo '#1!/bin/bash' >> meu-primeiro-bash = criando um bash

echo clear >> meu-primeiro-bash = add o limpar a tela

cat meu-primeiro-bash = ver o quem tem internamente


echo 'pdflatex aula\ 19-08.tex' >> meu-primeiro-bash  = tranferindo o .tex para meu-primeiro-bash

mv meu-primeiro-bash meu-primeiro-bash.sh = renomeando o arquivo

chmod +x meu-primeiro-bash.sh = implementando o .sh

./meu-primeiro-bash.sh  = executando

echo clear >> meu-primeiro-bash.sh = add mais um limpar a tela

cat meu-primeiro-bash.sh = mostrando conteudo








